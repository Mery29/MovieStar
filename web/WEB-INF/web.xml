<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Catalogos</servlet-name>
        <servlet-class>cr.ac.una.prograiv.moviestar.controller.CatalogosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Categorias</servlet-name>
        <servlet-class>cr.ac.una.prograiv.moviestar.controller.CategoriasServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Detalles</servlet-name>
        <servlet-class>cr.ac.una.prograiv.moviestar.controller.DetallesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MediaServlet</servlet-name>
        <servlet-class>cr.ac.una.prograiv.moviestar.controller.MediaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrdenesServlet</servlet-name>
        <servlet-class>cr.ac.una.prograiv.moviestar.controller.OrdenesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuariosServlet</servlet-name>
        <servlet-class>cr.ac.una.prograiv.moviestar.controller.UsuariosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Catalogos</servlet-name>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Categorias</servlet-name>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Detalles</servlet-name>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MediaServlet</servlet-name>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrdenesServlet</servlet-name>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuariosServlet</servlet-name>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
